{
  "name": "dapr-dashboard",
  "version": "0.2.0",
  "main": "main.js",
  "license": "MIT",
  "description": "A dashboard for all Dapr related needs.",
  "repository": "https://github.com/dapr/dashboard",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "e2e": "ng e2e",
    "watch": "tsc -w",
    "start:electron": "tsc main.ts && ng build && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "electron-postinstall": "electron-builder install-app-deps && ngcc",
    "postinstall": "node scripts/version.js"
  },
  "dependencies": {
    "@angular/animations": "~10.2.5",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~10.2.5",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/router": "~10.2.5",
    "@types/js-yaml": "^3.12.5",
    "init": "^0.1.2",
    "js-yaml": "^3.14.0",
    "lodash-es": "^4.17.15",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "monaco-editor": "^0.20.0",
    "ng-monaco-editor": "~2.2.3",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "web-animations-js": "github:angular/web-animations-js#release_pr208",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.3",
    "@angular/cli": "~10.2.3",
    "@angular/compiler-cli": "~10.2.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron": "^9.3.1",
    "electron-builder": "^22.7.0",
    "git-describe": "^4.0.4",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8"
  },
  "build": {
    "appId": "com.daprio.dapr-dashboard"
  },
  "husky": {
    "hooks": {
      "pre-push": "ng lint"
    }
  }
}
